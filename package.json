{
  "name": "bcrgui",
  "version": "0.0.0",
  "author": "Claudio Nicora",
  "homepage": "https://github.com/nicorac/bcr-gui",
  "scripts": {
    "ng": "ng",
    "start": "ionic serve",
    "build": "ng build",
    "build:android": "ionic capacitor build android --no-open --prod && cd android && gradlew assembleRelease && cd ..",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "generate-assets": "npx capacitor-assets generate --assets-path app/src/assets/icons --android",
    "open-android-studio": "npx cap open android"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^16.2.8",
    "@angular/common": "^16.2.1",
    "@angular/core": "^16.2.1",
    "@angular/forms": "^16.2.1",
    "@angular/platform-browser": "^16.2.1",
    "@angular/platform-browser-dynamic": "^16.2.1",
    "@angular/router": "^16.2.1",
    "@capacitor-community/contacts": "file:packages/capacitor-community-contacts-5.0.4.tgz",
    "@capacitor-community/keep-awake": "^4.0.0",
    "@capacitor-community/native-audio": "file:packages/capacitor-community-native-audio-5.0.1.tgz",
    "@capacitor/android": "^5.2.3",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.2.3",
    "@capacitor/filesystem": "^5.1.2",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/share": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capgo/capacitor-navigation-bar": "^6.0.6",
    "@ionic/angular": "^7.3.0",
    "capacitor-native-settings": "^5.0.1",
    "filesize": "^10.0.12",
    "ionicons": "^7.1.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.6.2",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.0.0",
    "@angular-eslint/eslint-plugin": "^16.0.0",
    "@angular-eslint/eslint-plugin-template": "^16.0.0",
    "@angular-eslint/template-parser": "^17.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler": "^16.2.1",
    "@angular/compiler-cli": "^16.2.1",
    "@angular/language-service": "^16.2.1",
    "@capacitor/assets": "^3.0.4",
    "@capacitor/cli": "^5.2.3",
    "@ionic/angular-toolkit": "^10.0.0",
    "@types/jasmine": "^4.3.0",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsdoc": "^46.4.6",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "description": "BCR-GUI is a companion app for BCR (Basic Call Recorder)"
}
